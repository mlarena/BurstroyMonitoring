        border-bottom: 1px solid #003d99;
    }
    
    .filter-link--active {
        color: #28a745;
        border-bottom-color: #28a745;
        font-weight: 500;
    }
    
    .filter-link--active:hover {
        color: #218838;
        border-bottom-color: #218838;
    }
    
    .filter-active-indicator {
        font-size: 12px;
        margin-left: 2px;
    }
</style>

@section Scripts {
    <script src="~/js/filter.js"></script>
}